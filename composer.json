{
    "name": "danstuchbury/laravel-log-enhancer",
    "description": "Enhancer for default Laravel Monolog",
    "type": "library",
    "require": {
        "php": "php >7.2",
        "illuminate/support": "5.8.*",
        "monolog/monolog": "1.23.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Stuchbury",
            "email": "dan.stuchbury@icloud.com"
        }
    ],
    "minimum-stability": "dev"
    "autoload": {
        "psr-4": {
            "Dstuchbury\\LaravelLogEnhancer": "src/"
        }
    },
    "extra": {
    "laravel": {
        "providers": [
            "Dstuchbury\\LaravelLogEnhancer\\LaravelLogEnhancerServiceProvider"
        ]
    }
}
}
